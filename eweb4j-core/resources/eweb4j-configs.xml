<?xml version="1.0" encoding="UTF-8"?>
<eweb4j>
	<configurations>
		<configuration id="eweb4j-core">
			<properties>
				<property name = "root_path" value="${root_path}" />
				<property name = "author" value="weiwei" />
			</properties>
		</configuration>
		
		<configuration id="eweb4j-mvc">
			<properties>
				<property name = "view_path" value="${root_path}/views" />
			</properties>
		</configuration>
		
		<configuration id="eweb4j-jdbc" type="properties">
			<file path="db.properties" /> 
		</configuration>
	</configurations>
</eweb4j>
